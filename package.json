{
  "name": "selenium-javascript",
  "version": "1.0.0",
  "description": "Automation Testing UI and API with Selenium and JavaScript",
  "main": "index.js",
  "scripts": {
    "test:fe": "mocha --recursive --timeout 10000 fe/fe_tests.js",
    "build": "npm run clean:dist && tsc",
    "check": "npm-check",
    "compile": "npm install && tsc",
    "recompile": "npm install && npm run clean:dist && tsc",
    "clean:dist": "rimraf dist/",
    "clean:reports": "rimraf be-e2e-report/ fe-e2e-report/",
    "clean:modules": "rm -rf node_modules/",
    "clean:uploads": "rimraf uploads/*.pdf",
    "clean:downloads": "rimraf download/*.pdf",
    "clean:all": "npm run clean:dist && npm run clean:reports && npm run clean:modules",
    "clean:local": "npm install && npm run clean:dist && npm run clean:reports && npm run clean:modules && npm run clean:uploads && npm run clean:downloads",
    "driver:version": "webdriver-manager version",
    "driver:update": "webdriver-manager update",
    "driver:status": "webdriver-manager status",
    "driver:clean": "webdriver-manager clean",
    "driver:ptor:status": "cross-env ./node_modules/protractor/node_modules/webdriver-manager/bin/webdriver-manager status",
    "driver:ptor:clean": "cross-env ./node_modules/protractor/node_modules/webdriver-manager/bin/webdriver-manager clean",
    "driver:ptor:latest": "cross-env ./node_modules/protractor/node_modules/webdriver-manager/bin/webdriver-manager update --gecko=false --appium=false --android=false --ie=false --versions.chrome=latest",
    "driver:flake:status": "cross-env ./node_modules/protractor-flake/node_modules/webdriver-manager/bin/webdriver-manager status",
    "driver:flake:clean": "cross-env ./node_modules/protractor-flake/node_modules/webdriver-manager/bin/webdriver-manager clean",
    "driver:flake:latest": "cross-env ./node_modules/protractor-flake/node_modules/webdriver-manager/bin/webdriver-manager update --gecko=false --appium=false --android=false --ie=false --versions.chrome=latest",
    "driver:setup:ci": "npm run driver:ptor:chrome76 && npm run driver:ptor:status && npm run driver:flake:chrome76 && npm run driver:flake:status",
    "driver:setup:local": "npm run driver:ptor:chrome78 && npm run driver:ptor:status && npm run driver:flake:chrome78 && npm run driver:flake:status",
    "driver:setup:latest": "npm run driver:ptor:latest && npm run driver:ptor:status && npm run driver:flake:latest && npm run driver:flake:status",
    "driver:cleanup": "npm run driver:ptor:clean && npm run driver:flake:clean",
    "lint": "eslint . --ext ts --max-warnings 0",
    "lint:fix": "eslint .  --ext ts --fix --max-warnings 0"
  },
  "repository": "git@github.com:dalenguyen/selenium-javascript.git",
  "author": "Darko Cvetkovic <darkocvetkovicwb22@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@types/chai": "^4.2.12",
    "@types/fs-extra": "^9.0.1",
    "@types/jasmine": "^3.5.12",
    "@types/jest": "^26.0.9",
    "@types/jest-expect-message": "^1.0.2",
    "@types/node": "^14.0.27",
    "@types/request-promise": "^4.1.46",
    "@types/selenium-webdriver": "^4.0.9",
    "@types/uuid": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "ajv": "^6.12.3",
    "auth0": "^2.28.0",
    "axios": "^0.19.2",
    "axios-retry": "^3.1.8",
    "base64-url": "^2.3.3",
    "chai": "^4.2.0",
    "chai-smoothie": "^0.3.2",
    "chromedriver": "^87.0.2",
    "cross-env": "^7.0.2",
    "csvtojson": "^2.0.10",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "form-data": "^3.0.0",
    "fs-extra": "^9.0.1",
    "i18n-iso-countries": "^6.0.0",
    "jasmine": "^3.6.1",
    "jest": "^26.3.0",
    "jest-circus": "^26.3.0",
    "jest-expect-message": "^1.0.2",
    "jest-html-reporter": "^3.1.3",
    "js-yaml": "^3.14.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "lz-string": "1.4.4",
    "mime-kind": "^3.0.0",
    "moment": "^2.27.0",
    "moment-business-days": "^1.2.0",
    "node-html-parser": "^1.2.20",
    "npm-check": "^5.9.2",
    "p-limit": "^3.0.2",
    "path": "^0.12.7",
    "pdfkit": "^0.11.0",
    "prettier": "^2.1.1",
    "protractor": "^7.0.0",
    "protractor-beautiful-reporter": "^1.3.6",
    "protractor-flake": "^4.0.0",
    "qs": "^6.9.4",
    "reflect-metadata": "^0.1.13",
    "request-promise": "^4.2.6",
    "rimraf": "^3.0.2",
    "selenium-webdriver": "^4.0.0-alpha.8",
    "sync-request": "^6.1.0",
    "ts-jest": "^26.1.4",
    "ts-node": "8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.7",
    "typescript-eslint-parser": "^22.0.0",
    "typescript-string-operations": "^1.3.3",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "mocha": "^5.2.0"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
